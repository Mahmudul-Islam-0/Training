@model List<Department>


@{
    ViewData["Title"] = "Home Page";
}




<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>

     

    @foreach(var item in Model){
        <h1>@item.CreatedDate</h1>
        <h1>@item.Name</h1>
        <h1>@item.Id</h1>
        <h1><a asp-action="DepartmentSave" asp-controller="Home"  asp-route-Id="@item.Id" class="btn btn-primary">link @item.Id</a></h1>
        <h1><a asp-action="DepartmentDelete" asp-controller="Home"  asp-route-Id="@item.Id" class="btn btn-primary">Delete</a></h1>
        <h1>@item.UpdatedBy</h1>
        <h1>@item.IsDeleted</h1>
    }

</div>
